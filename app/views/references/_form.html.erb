

<%= simple_form_for [@agency, @reference] do |f| %>
  <%= f.error_notification %>
  <div class="form-inputs">

  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
    </div>
  <div class="form-actions">
  <%= f.button :submit %>
    </div>
<% end %>


